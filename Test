cp /path/to/your/downloaded/grub2-efi-x64-modules-2.12-32.fc42.noarch.rpm /tmp/

cd /tmp
rpm -Uvh --force grub2-efi-x64-modules-2.12-32.fc42.noarch.rpm

ls /usr/lib/grub/x86_64-efi/modinfo.sh

grub2-install --target=x86_64-efi --efi-directory=/boot/efi --bootloader-id=Nobara --recheck

grub2-mkconfig -o /boot/grub2/grub.cfg
